{
	"manifest_version": 3,

	"name": "Bread",
	"description": "Easy video downloading!",
	"version": "1.2",

	"action": {
		"default_icon": {
			"16": "./assets/icon.png",
			"32": "./assets/icon.png"
		},
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "./assets/icon.png",
		"32": "./assets/icon.png",
		"48": "./assets/icon.png",
		"128": "./assets/icon.png"
	},
	"permissions": [
		"activeTab",
		"downloads"
	]
}
